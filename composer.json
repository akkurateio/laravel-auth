    },
    "require": {
        "akkurate/laravel-core": "^2.0",
        "akkurate/laravel-back-components": "^2.0",
        "laravel/passport": "^10.0",
        "laravel/ui": "^2.0|^3.0",
        "thomaswelton/laravel-gravatar": "^1.2",
        "spatie/laravel-web-tinker": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "doctrine/dbal": "^3.0"
    },
    "autoload" : {
        "psr-4": {
            "Akkurate\\LaravelAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akkurate\\LaravelAuth\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [

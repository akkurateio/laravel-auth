{
    "name": "akkurateio/laravel-auth",
    "description": "Auth logic for Akkurate Ecosystem",
    "type": "library",
    "license": "AGPL-3.0-only",
    "authors": [
        {
            "name": "Akkurate",
            "email": "hello@subvitamine.com",
            "homepage": "https://www.akkurate.io",
            "role": "Company"
        }
    ],
    "support": {
        "docs": "https://github.com/akkurateio/documentation"
    },
    "require": {
        "akkurateio/laravel-core": "^0.1",
        "akkurateio/laravel-back-components": "^0.1",
        "laravel/passport": "^10.0",
        "laravel/ui": "^2.0|^3.0",
        "thomaswelton/laravel-gravatar": "^1.2",
        "spatie/laravel-web-tinker": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "doctrine/dbal": "^3.0"
    },
    "autoload" : {
        "psr-4": {
            "Akkurate\\LaravelAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akkurate\\LaravelAuth\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Akkurate\\LaravelAuth\\LaravelAuthServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
